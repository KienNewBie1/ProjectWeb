@model IEnumerable<ProjectWeb.Models.Categories>
<!--category-productsr-->
@foreach (var item in Model)
{
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4 class="panel-title"><a href="@Url.Action("ProductById","Home",new {id = item.Id})">@item.Name</a></h4>
        </div>
    </div>
}
<!--/category-products-->